import processing.video.*;

Movie myMovie;

void setup()
{
  size(640,480,P2D);
  myMovie = new Movie(this,"andrine.avi");
  myMovie.play();
}

void draw()
{
  image(myMovie,0,0,640,480);
}

void keyPressed()
{
  if(key == 'h') {
    myMovie.jump(28);
  }
  if(key == 's') {
    myMovie.jump(36);
  }
  if(key == 'a') {
    myMovie.jump(48);
  }
  if(key == 'l') {
    myMovie.jump(61);
  }
}
