import processing.video.*;

Movie[] myMovie = new Movie[2];

void setup()
{
  size(1280,528,P2D);
  
  myMovie[0] = new Movie(this,"misample.avi");
  myMovie[1] = new Movie(this,"misample2.avi");

}

void draw(){
  image(myMovie[0],0,0,1280,528);
}

void keyPressed(){
  
  
  if(key == 'a') {
    myMovie[0].stop();
    myMovie[0].play();
  }
}

void movieEvent (Movie m)
{
  m.read();
}
